{
  "name": "agente-digital-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <23.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "@rollup/rollup-win32-x64-msvc": "^4.44.2"
  },
  "scripts": {
    "dev": "vite",
    "safe-dev": "npm run backup && npm run dev",
    "dev-alt": "node node_modules/vite/bin/vite.js",
    "build": "vite build",
    "preview": "vite preview",
    "start": "concurrently \"cd .. && python3 run_fixed.py\" \"npm run dev\"",
    "start:full": "concurrently --kill-others-on-fail \"cd .. && python3 run_fixed.py\" \"npm run dev\"",
    "backend": "cd .. && python3 run_fixed.py",
    "frontend": "npm run dev",
    "backup": "echo Backup skipped - git not in PATH",
    "restore": "git reset --hard HEAD~1",
    "safe-install": "rm -rf node_modules package-lock.json && npm ci",
    "health-check": "npm audit && npm outdated",
    "fix-deps": "npm run safe-install && npm run dev",
    "test": "vitest run || true"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.16",
    "@heroicons/vue": "2.0.18",
    "@sentry/vue": "7.78.0",
    "@vueuse/motion": "2.0.0",
    "animate.css": "4.1.1",
    "apexcharts": "3.44.0",
    "axios": "^1.7.2",
    "chart.js": "4.4.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-vue-next": "0.292.0",
    "pinia": "2.1.7",
    "primeicons": "6.0.1",
    "primevue": "3.46.0",
    "sweetalert2": "^11.22.2",
    "vue": "3.3.8",
    "vue-chartjs": "5.2.0",
    "vue-router": "4.2.5",
    "vue3-apexcharts": "1.4.4",
    "web-vitals": "3.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.4.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "esbuild": "0.19.5",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "imagemin-webp": "^8.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "rollup": "3.29.4",
    "rollup-plugin-gzip": "^3.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.3",
    "vite": "4.5.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vitest": "^1.3.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.44.2"
  }
}
